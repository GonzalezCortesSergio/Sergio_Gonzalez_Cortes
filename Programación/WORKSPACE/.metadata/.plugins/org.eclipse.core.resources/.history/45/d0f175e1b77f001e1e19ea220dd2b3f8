package ejercicio05;



public class Copiado {

	//Atributos
	
	String texto;
	int numero;
	
	//Constructor
	
	public Copiado (String texto, int numero) {
		
		this.texto = texto;
		this.numero = numero;
		
	}
	
	//Getter
	
	public String getTexto () {
		
		return texto;
	}
	
	public int getNumero () {
		
		return numero;
	}
	
	//MÃ©todos
	
	public void mostrarTexto (String texto) {
		
		System.out.println(texto);
		
	}
	
	
}
